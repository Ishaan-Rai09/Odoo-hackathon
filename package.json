{
  "name": "elite-events-platform",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "database"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "install:all": "npm install && npm run install:modules",
    "install:modules": "npm install --workspace=frontend && npm install --workspace=backend && npm install --workspace=database",
    "build:all": "npm run build --workspace=database && npm run build --workspace=backend && npm run build --workspace=frontend && npm run build",
    "dev:database": "npm run dev --workspace=database",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend"
  },
